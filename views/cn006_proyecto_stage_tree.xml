<odoo>
    <record id="cn006_proyecto_etapa_view_tree" model="ir.ui.view">
        <field name="name">(CN006) Proyecto - Etapas - tree</field>
        <field name="model">project.project.stage</field>
        <field name="priority" eval="100"/>
        <field name="arch" type="xml">


            <tree editable="top" sample="1">
                <field name="sequence" widget="handle"/>
                <field name="name"/>
                <field name="cn006_stage"/>
                <field name="mail_template_id" optional="hide" context="{'default_model': 'project.project'}"/>
                <field name="fold" optional="show"/>
            </tree>
        </field>
    </record>
</odoo>
